      "userId": "05735385076343474033"
     },
     "user_tz": -480
    },
    "id": "rkIidcNiuRm_",
    "outputId": "4c1bdb7f-8347-4c30-d72d-0bca5bbd3a52"
   },
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "10969088 10\n",
      "139892257508856 test\n",
      "139892257508856 test\n",
      "139892257508856 test\n"
     ]
    },
    {
     "data": {
      "text/plain": [
       "True"
      ]
     },
     "execution_count": 20,
     "metadata": {
      "tags": []
