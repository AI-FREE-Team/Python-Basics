   ],
   "source": [
    "x = 10\n",
    "y = \"test\"\n",
    "print(id(x),x)\n",
    "print(id(y),y)\n",
    "x = y\n",
    "print(id(x),x)\n",
    "print(id(y),y)"
   ]
